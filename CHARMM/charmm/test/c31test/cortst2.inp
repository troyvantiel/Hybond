! setup images and nonbonded options
crystal define cubi 15.3 15.3 15.3 90 90 90
crystal build cutoff 6 nope
image byres xcen 0.0 ycen 0.0 zcen 0.0 sele all end

update inbfrq 1 imgfrq 1

energy inbfrq -1 imgfrq -1 ihbfrq 0 atom vatom vswitch -
      cutnb 6 cutim 6 ctofnb 5 ctonnb 4 eps 1.0 -
      ewald kapp 0.4 kmax 6 ksqm 70 pmew spli orde 4 -
      FFTX 32 FFTY 32 FFTZ 32 qcor 0.

coor copy comp

shake bonh para


! RUN SHORT TRAJECTORY AND SAVE 
open unit 10 write file name @9cortst2.trj

dynamics VERLET strt timestep 0.001 nstep 200  -
      iprfrq 100 ihtfrq 0 ntrfrq 1000 ieqfrq 100 -
      iunrea -1 iunwri -1 iuncrd 10  kunit -1 iunvel -1  -
      nprint 50 nsavv -1 nsavc 1 inbfrq -1 ihbfrq 0 -
      firstt 285.0 finalt 285.0 teminc 0.0 isvfrq 0 -
      iasors 0 iasvel 1 iscvel 0 ichecw 0 twindh 5.0 twindl -5.0

if ?nocorrel .eq. 1 then stop

if ?shell .ne. 1 then goto noshell
shell nshl 1 shthk 3.75 -
      solute  select resname ala  end -
      solvent select resname tip3 end
label noshell

open unit 10 read file name @9cortst2.trj

open write card unit 80 name @9cor_vecm.dat
open write card unit 81 name @9cor_vect.dat
open write card unit 82 name @9cor_dipo_ala.dat
open write card unit 83 name @9cor_dipo_wat.dat
if ?shell .ne. 1 then goto noshell2
open write card unit 84 name @9cor_sdip_1.dat
label noshell2

correl  maxt 200 maxs 20
enter vecm vecm wat 71 OH2 ala1 1 CA 
enter vect vect xyz wat 71 OH2 ala1 1 CA 
enter dip1 dipo select resname ala end
enter dip2 dipo select resname tip3 end
if ?shell .ne. 1 then goto noshell3
enter sdip sdip shell 1
label noshell3

show all

traj  firstu 10 nunit 1 begin 1 stop 200 skip 1

show all

write vecm unit 80 dumb time
write vect unit 81 dumb time
write dip1 unit 82 dumb time
write dip2 unit 83 dumb time
if ?shell .ne. 1 then goto noshell4
write sdip unit 84 dumb time
label noshell4

end

STOP
