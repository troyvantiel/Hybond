if ?flexparm .ne. 1 then
 echo "Test NOT performed."
 stop
endif

stream datadir.def
bomlev -2

!!!!! 1 first section
set return fromtop19
goto readtop19
label fromtop19

read parameter card name @0param19.prm

READ  SEQUENCE  CARDS
* PTI SEQUENCE
*
   58
ARG PRO ASP PHE CYS LEU GLU PRO PRO TYR
THR GLY PRO CYS LYS ALA ARG ILE ILE ARG
TYR PHE TYR ASN ALA LYS ALA GLY LEU CYS
GLN THR PHE VAL TYR GLY GLY CYS ARG ALA
LYS ARG ASN ASN PHE LYS SER ALA GLU ASP
CYS MET ARG THR CYS GLY GLY ALA

GENERATE  MAIN
PATCH DISU MAIN  5 MAIN 55
PATCH DISU MAIN 14 MAIN 38
PATCH DISU MAIN 30 MAIN 51

READ  SEQUENCE  WATER  4
GENERATE  SOLV  NOANGLES

open unit 3 read card name @0bpti.crd
READ COORD card  UNIT 3
close unit 3

HBONDS ALL IHBFRQ 50
ENERGY ATOM CDIE SHIF  VSHI VDIS
set efirst ?ener
coor force comp

open unit 3 write card  name @9/flexpartest.frc
write coor card comp UNIT 3
* forces for flexible paramter I/O test
*

write psf card xplor name @9/flexpartest.psf

delete atom select all end

read rtf card name @0/toph19.rtf
read param card name @0/param19.prm

read psf card xplor name @9/flexpartest.psf

open unit 3 read card name @0bpti.crd
READ COORD card  UNIT 3
close unit 3

HBONDS ALL IHBFRQ 50
ENERGY ATOM CDIE SHIF  VSHI VDIS
set epsf ?ener
coor force comp

calc ediff abs ( ?ener - @efirst )
if @ediff .gt. 0.00001 then
      echo failed test1a
else
      echo passed test1a
endif

read coor card name @9/flexpartest.frc
coor diff
coor dist weigh
print coor sele prop wmain .gt. 0.00001 end
if ?nsel gt 0 then
      echo failed test1b
else
      echo passed test1b
endif

delete atom select all end

!!!!! 2 second section
! read in a "bad" rtf 

READ RTF CARD
* TOPOLOGY FILE FOR PROTEINS USING EXPLICIT HYDROGEN ATOMS: VERSION 19
* - This is a nearly null set for testing only...
*
   20    1                ! Version number
MASS     1 H      1.00800 ! hydrogen which can h-bond to neutral atom
END

set return = fromflxpar1
goto flxpar1
label fromflxpar1

read psf card xplor name @9/flexpartest.psf
read coor card name @0/bpti.crd

HBONDS ALL IHBFRQ 50
ENERGY ATOM CDIE SHIF  VSHI VDIS

coor force comp
calc ediff abs ( ?ener - @efirst )

if @ediff .gt. 0.00001 then
      echo failed test2a
else
      echo passed test2a
endif

read coor card name @9/flexpartest.frc
coor diff
coor dist weigh
print coor sele prop wmain .gt. 0.00001 end
if ?nsel gt 0 then
      echo failed test2b
else
      echo passed test2b
endif

delete atom select all end

!!!!! 3 third section
! now read the paramter file two parts
! test APPENd feature...
!

read rtf card name @0/toph19.rtf
read parameter card flex print name @0/flexpartest_xh.prm

set return = fromreadh
goto readh
label fromreadh

read psf card xplor name @9/flexpartest.psf
read coor card name @0/bpti.crd

HBONDS ALL IHBFRQ 50
ENERGY ATOM CDIE SHIF  VSHI VDIS

coor force comp
calc ediff abs ( ?ener - @efirst )

