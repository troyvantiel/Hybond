coor dist weigh
print coor sele prop wmain .gt. 0.00001 end
if ?nsel gt 0 then
      echo failed test3b
else
      echo passed test3b
endif

!!!!! 4 fourth section
! test reading/writing various psf files
!

write psf card oldpsf name @9/psfold.psf  ! old format psf w/ IAC in psf
write psf card name @9/psf.psf             ! "new" default xplor-like format with ATC(IAC(I)) in psf
delete atom select all end

! PSFOLD
read psf card name @9/psfold.psf
read coor card name @0/bpti.crd

HBONDS ALL IHBFRQ 50
ENERGY ATOM CDIE SHIF  VSHI VDIS

coor force comp
calc ediff abs ( ?ener - @efirst )

if @ediff .gt. 0.00001 then
      echo failed test4a
else
      echo passed test4a
endif

read coor card name @9/flexpartest.frc
coor diff
coor dist weigh
print coor sele prop wmain .gt. 0.00001 end
if ?nsel gt 0 then
      echo failed test4b
else
      echo passed test4b
endif

delete atom select all end

! "New" default xplor-like PSF
read psf card name @9/psf.psf
read coor card name @0/bpti.crd

HBONDS ALL IHBFRQ 50
ENERGY ATOM CDIE SHIF  VSHI VDIS

coor force comp
calc ediff abs ( ?ener - @efirst )

if @ediff .gt. 0.00001 then
      echo failed test5a
else
      echo passed test5a
endif

read coor card name @9/flexpartest.frc
coor diff
coor dist weigh
print coor sele prop wmain .gt. 0.00001 end
if ?nsel gt 0 then
      echo failed test5b
else
      echo passed test5b
endif

delete atom select all end

stop

